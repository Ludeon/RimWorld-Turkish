  <TerribleParty.stages.terrible_party.label>korkunç parti</TerribleParty.stages.terrible_party.label>
  <!-- EN: That party was terrible. Bad music, awkward dancing... ugh. -->
  <TerribleParty.stages.terrible_party.description>O parti korkunçtu. Kötü müzik, garip danslar... ah.</TerribleParty.stages.terrible_party.description>
  
  <!-- EN: terrible sacrifice -->
  <TerribleSacrifice.stages.terrible_sacrifice.label>korkunç kurban</TerribleSacrifice.stages.terrible_sacrifice.label>
