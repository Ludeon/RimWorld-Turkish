<?xml version="1.0" encoding="UTF-8"?>
<LanguageData>
  
  <!-- EN: harp -->
  <Harp.label>arp</Harp.label>
  <!-- EN: Craft the harp, a simple stationary musical instrument popular among nobility in some cultures. -->
  <Harp.description>Bazı kültürlerde soylular arasında popüler bir basit, sabit müzik aleti olan arp yap.</Harp.description>
  <!-- EN:
    <li>subject->harps</li>
    <li>subject->simple string instruments</li>
    <li>subject_story->trained in the school of music</li>
    <li>subject_story->constructed a harp from common feral world materials</li>
    <li>subject_story->performed a harp solo for the Imperial nobility</li>
    <li>subject_story->practiced glissandos every day</li>
    <li>subject_story->researched the cultural significance of stringed instrument usage</li>
    <li>subject_gerund->playing the harp</li>
  -->
  <Harp.generalRules.rulesStrings>
    <li>subject->arplar</li>
    <li>subject->telli basit çalgılar</li>
    <li>subject_story->müzik okulunda eğitim aldı</li>
    <li>subject_story->yabanda yaygın malzemelerden bir arp inşa etti</li>
    <li>subject_story->İmparatorluk soylularına arp solosu icra etti</li>
    <li>subject_story->her gün glissando çalıştı</li>
    <li>subject_story->telli çalgı kullanımının kültürel önemini araştırdı</li>
    <li>subject_gerund->arp çalma</li>
  </Harp.generalRules.rulesStrings>
  
  <!-- EN: harpsichord -->
  <Harpsichord.label>klavsen</Harpsichord.label>
  <!-- EN: Craft the harpsichord, a complex stationary musical instrument. -->
  <Harpsichord.description>Karmaşık bir sabit müzik aleti olan klavsen yap.</Harpsichord.description>
  <!-- EN:
    <li>subject->harpsichord</li>
    <li>subject_story->led a renaissance in the harpsichord in the coreworld systems</li>
    <li>subject_story->tuned harpsichords for concert use on Glitterworlds</li>
    <li>subject_story->took up a role as lead harpischordist for the local philharmonic</li>
    <li>subject_story->constructed dual-keyboard harpsichords focused on harmonics</li>
    <li>subject_gerund->playing the harpsichord</li>
  -->
  <Harpsichord.generalRules.rulesStrings>
    <li>subject->klavsen</li>
    <li>subject_story->merkezdünya sistemlerinde klavsen sanatında bir rönesans başlattı</li>
    <li>subject_story->ışıltıdünyalarda konser kullanımı için klavsenleri akort etti</li>
    <li>subject_story->yerel filarmoniye baş klavsenist olarak katıldı</li>
    <li>subject_story->armoniye odaklı çift klavyeli klavsenler inşa etti</li>
    <li>subject_gerund->klavsen çalma</li>
  </Harpsichord.generalRules.rulesStrings>
  
  <!-- EN: piano -->
  <Piano.label>piyano</Piano.label>
  <!-- EN: Craft the piano, an advanced stationary musical instrument. -->
  <Piano.description>Gelişmiş bir sabit müzik aleti olan piyano yap.</Piano.description>
  <!-- EN:
    <li>subject->piano</li>
    <li>subject_story->was acquainted with the entire corpus of stringed instruments</li>
    <li>subject_story->joined a music academy, where the students learned about composers as much as the piano itself</li>
    <li>subject_story->transported pianos between the coreworlds</li>
    <li>subject_story->recorded the techniques of several master pianists</li>
    <li>subject_story->constructed a strange piano</li>
    <li>subject_gerund->playing the piano</li>
  -->
  <Piano.generalRules.rulesStrings>
    <li>subject->piyano</li>
    <li>subject_story->telli çalgıların tüm külliyatına hâkimdi</li>
    <li>subject_story->bir müzik akademisine katıldı; burada öğrenciler piyanoyu olduğu kadar bestecileri de öğrendi</li>
    <li>subject_story->merkezdünyalar arasında piyanolar taşıdı</li>
    <li>subject_story->birkaç usta piyanistin tekniklerini kayıt altına aldı</li>
    <li>subject_story->tuhaf bir piyano inşa etti</li>
    <li>subject_gerund->piyano çalma</li>
  </Piano.generalRules.rulesStrings>
  
</LanguageData>