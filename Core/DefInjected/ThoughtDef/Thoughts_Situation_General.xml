  <!-- EN: chilly -->
  <EnvironmentCold.stages.chilly.label>serin</EnvironmentCold.stages.chilly.label>
  <!-- EN: I'm uncomfortably cold. -->
  <EnvironmentCold.stages.chilly.description>Rahat edemeyecek kadar üşüyorum.</EnvironmentCold.stages.chilly.description>
  <!-- EN: cold -->
  <EnvironmentCold.stages.cold.label>soğuk</EnvironmentCold.stages.cold.label>
  <!-- EN: I'm so cold, I'm shivering. -->
  <EnvironmentCold.stages.cold.description>Çok üşüyorum, titriyorum.</EnvironmentCold.stages.cold.description>
  <!-- EN: numbing cold -->
  <EnvironmentCold.stages.numbing_cold.label>uyuşturan soğuk</EnvironmentCold.stages.numbing_cold.label>
  <!-- EN: I can't stop shivering. My teeth are chattering. All I can think of is getting warm. -->
  <EnvironmentCold.stages.numbing_cold.description>Titremeyi durduramıyorum. Dişlerim birbirine çarpıyor. Tek düşünebildiğim ısınmak.</EnvironmentCold.stages.numbing_cold.description>
  <!-- EN: absolutely freezing -->
  <EnvironmentCold.stages.absolutely_freezing.label>dondurucu soğuk</EnvironmentCold.stages.absolutely_freezing.label>
  <!-- EN: My teeth are chattering so hard I can barely speak. My toes are numb. I need to get warm. -->
  <EnvironmentCold.stages.absolutely_freezing.description>Dişlerim birbirine o kadar çok çarpıyor ki zorlukla konuşabiliyorum. Ayak parmaklarım uyuşmuş. Isınmam lazım</EnvironmentCold.stages.absolutely_freezing.description>
  
  <!-- EN: darkness -->
  <EnvironmentDark.stages.darkness.label>karanlık</EnvironmentDark.stages.darkness.label>
