<?xml version="1.0" encoding="UTF-8"?>
<LanguageData>
  
  <!-- EN: go frenzy -->
  <Frenzy_Go.label>çılgın koşucu</Frenzy_Go.label>
  <!-- EN: Inspired: Go frenzy -->
  <Frenzy_Go.baseInspectLine>İlham aldı: Çılgın koşucu</Frenzy_Go.baseInspectLine>
  <!-- EN: [PAWN_pronoun] will move faster for the next 8 days. -->
  <Frenzy_Go.beginLetter>O, önümüzdeki 8 gün boyunca daha hızlı hareket edecek.</Frenzy_Go.beginLetter>
  <!-- EN: {0} is no longer inspired (go frenzy). -->
  <Frenzy_Go.endMessage>{0} ilhamını kaybetti (çılgın koşucu).</Frenzy_Go.endMessage>
  
